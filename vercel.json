{
    "version": 2,
    "builds": [
      { "src": "*.html", "use": "@vercel/static" },
      { "src": "dist/**", "use": "@vercel/static" },
      { "src": "styles/**", "use": "@vercel/static" },
      { "src": "img/**", "use": "@vercel/static" },
      { "src": "video/**", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/" }
    ]
  }